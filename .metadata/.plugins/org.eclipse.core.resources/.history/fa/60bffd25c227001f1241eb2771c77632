package demo;

import java.util.Scanner;

public class Array2 {

	public static void main(String[] args) {

		int demo[]=new int[3];
		Scanner sc=new Scanner(System.in);
		
		for(int i=0; i<demo.length; i++)
		{
			System.out.println("Enter the" + i + " "  + "element");
			demo[i]=sc.nextInt();
		}
		
		for(int i: demo)
		{
			System.out.println(i);
		}
	}

}
