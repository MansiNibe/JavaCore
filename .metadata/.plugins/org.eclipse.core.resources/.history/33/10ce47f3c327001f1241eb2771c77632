package demo;

import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {

		Scanner sc=new Scanner(System.in);
		System.out.println("Please enter the number you want to check...");
		int num=sc.nextInt();
		boolean flag=true;
		for(int i=2; i<num; i++)
		{
			if(num%i==0)
			{
				flag=false;
			}
				
		}
		
	}

}
